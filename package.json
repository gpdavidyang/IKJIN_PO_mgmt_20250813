  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "vite build && cp -r fonts dist/ && esbuild server/production.ts --platform=node --packages=external --bundle --format=esm --outdir=dist --external:vite --external:rollup --external:@rollup/* --external:esbuild",
    "start": "NODE_ENV=production node dist/production.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:unit": "NODE_ENV=test jest --testPathPattern='(unit|spec)'",
    "test:integration": "NODE_ENV=test jest --testPathPattern='integration'",
    "test:e2e": "NODE_ENV=test jest --testPathPattern='e2e'",
    "test:workflow": "NODE_ENV=test jest --testPathPattern='(unified-workflow|horizontal-progress|use-order-workflow)'",
    "test:workflow:watch": "NODE_ENV=test jest --watch --testPathPattern='(unified-workflow|horizontal-progress|use-order-workflow)'",
    "test:workflow:e2e": "playwright test tests/e2e/unified-workflow.e2e.test.ts",
    "test:all": "./tests/run-tests.sh",
    "test:playwright": "playwright test",
    "test:playwright:local": "TEST_ENV=local playwright test",
    "test:playwright:prod": "TEST_ENV=production playwright test",
    "test:playwright:ui": "playwright test --ui",
    "test:playwright:debug": "playwright test --debug",
    "test:playwright:headed": "playwright test --headed",
    "test:playwright:report": "playwright show-report",
    "test:order-status": "NODE_ENV=test jest server/tests/order-status-simple.test.js",
    "test:order-workflow": "TEST_ENV=local playwright test tests/e2e/order-creation-workflow.spec.ts",
    "migrate:order-status": "npx tsx server/scripts/migrate-order-status.ts",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "build:analyze": "npm run build && npm run analyze:bundle",
    "bundle:size": "npm run build && node scripts/analyze-bundle.js",
    "dashboard:help": "node scripts/test-dashboard-simple.js help"
  },
