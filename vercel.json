{
  "version": 2,
  "routes": [
    {
      "src": "/api/test",
      "dest": "/api/test.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/[...all].js"
    },
    {
      "src": "/fonts/(.*)",
      "dest": "fonts/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "assets/$1"
    },
    {
      "src": "/PO_Excel_Template\\.xlsx",
      "dest": "PO_Excel_Template.xlsx",
      "headers": {
        "Content-Type": "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet",
        "Content-Disposition": "attachment; filename=\"PO_Excel_Template.xlsx\""
      }
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|svg|ico|woff|woff2|ttf|otf|xlsx|json))",
      "dest": "$1"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ],
  "functions": {
    "api/[...all].js": {
      "maxDuration": 60
    }
  },
  "build": {
    "env": {
      "PLAYWRIGHT_SKIP_BROWSER_DOWNLOAD": "1",
      "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true"
    }
  },
  "env": {
